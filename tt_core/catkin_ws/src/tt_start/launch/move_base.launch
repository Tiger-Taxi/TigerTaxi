        <param name="base_global_planner" type="string" value="navfn/NavfnROS"/>

        <!-- MOVE_BASE YAML PARAMETERS -->

        <rosparam file="$(find tt_start)/config/costmap_common_params.yaml" command="load" ns="global_costmap"/>
        <rosparam file="$(find tt_start)/config/costmap_common_params.yaml" command="load" ns="local_costmap"/>

        <rosparam file="$(find tt_start)/config/global_costmap_params.yaml" command="load"/>

        <rosparam file="$(find tt_start)/config/local_costmap_params.yaml" command="load"/>


